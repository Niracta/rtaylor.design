  "resolutions": {
    "graphql": "^14.6.0",
    "cypress": "^4.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --config=svgo.yml --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
