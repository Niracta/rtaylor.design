    "typeface-montserrat": "0.0.75",
    "typeface-work-sans": "^0.0.72",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
