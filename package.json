{
  "name": "portfolio-template",
  "private": true,
  "author": "Drake Costa <drake@saeris.io> (https://github.com/Saeris/)",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Saeris/Gatsby-Portfolio-Template"
  },
  "bugs": {
    "url": "https://github.com/Saeris/Gatsby-Portfolio-Template/issues"
  },
  "scripts": {
    "build": "run-s build:**",
    "build:app": "gatsby build",
    "build:lambda": "cross-env NODE_ENV=`production` LAMBDA=true netlify-lambda build src/lambda --config ./webpack.functions.js",
    "dev": "yarn start",
    "start": "run-p -l start:**",
    "start:app": "cross-env CYPRESS_SUPPORT=y CHOKIDAR_USEPOLLING=true gatsby develop",
    "start:lambda": "cross-env LAMBDA=true npx --node-arg=--inspect netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write content/**/*.yaml",
    "type-check": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "server:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "server:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "server:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci",
    "test:compile": "tsc --project cypress/tsconfig.json",
    "test:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "test": "npm-run-all -s test:compile server:run",
    "test:watch": "npm-run-all -p test:compile:watch server:open",
    "test:ci": "npm-run-all -s test:compile server:ci"
  },
  "dependencies": {
