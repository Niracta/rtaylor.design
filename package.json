    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.14",
    "graphql": "^14.6.0",
    "nodemailer": "^6.4.3",
    "polished": "^3.4.4",
    "prism-react-renderer": "^1.0.2",
